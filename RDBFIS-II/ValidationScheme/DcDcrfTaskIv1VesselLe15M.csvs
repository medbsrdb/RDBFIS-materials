secondary_fishing_gear: any("NO","FCN","FG","GN","HMD","HMX","HPM","LN","LNB","LNP","LNS","OT","PS1","PS2","PT","RG","SDN","SPR","SSC","TBN","TBS","TMS","PS","SUX","LA","SB","SV","SX","TBB","PTB","TB","PTM","TM","OTP","OTB","OTM","TX","OTT","DRH","DRM","DRB","GTN","GND","GNC","GNF","GEN","GNS","GTR","FAR","FWR","FYK","FPO","FPN","FSN","FIX","LLD","LHP","LX","LL","LHM","LLS","LTL","LVT","MDV","MDR","MEL","MIS","MHI","HAR","MPM","MPN","MSP","NK") or empty
third_fishing_gear: any("NO","FCN","FG","GN","HMD","HMX","HPM","LN","LNB","LNP","LNS","OT","PS1","PS2","PT","RG","SDN","SPR","SSC","TBN","TBS","TMS","PS","SUX","LA","SB","SV","SX","TBB","PTB","TB","PTM","TM","OTP","OTB","OTM","TX","OTT","DRH","DRM","DRB","GTN","GND","GNC","GNF","GEN","GNS","GTR","FAR","FWR","FYK","FPO","FPN","FSN","FIX","LLD","LHP","LX","LL","LHM","LLS","LTL","LVT","MDV","MDR","MEL","MIS","MHI","HAR","MPM","MPN","MSP","NK") or empty
overall_length: range(2,15)
gross_tonnage: range(0,100)
engine_power_kw: range(0,350)
construction_year: switch(($reference_year/is("2013"),range(1860,2013)),($reference_year/is("2014"),range(1860,2014)),($reference_year/is("2015"),range(1860,2015)),($reference_year/is("2016"),range(1860,2016)),($reference_year/is("2017"),range(1860,2017)),($reference_year/is("2018"),range(1860,2018)),($reference_year/is("2019"),range(1860,2019)),($reference_year/is("2020"),range(1860,2020)),($reference_year/is("2021"),range(1860,2021)),($reference_year/is("2022"),range(1860,2022)),($reference_year/is("2023"),range(1860,2023))) or is("-999")
