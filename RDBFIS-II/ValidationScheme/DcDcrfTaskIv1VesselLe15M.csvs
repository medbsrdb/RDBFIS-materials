version 1.2
@totalColumns 43
@permitEmpty
@ignoreColumnNameCase
reference_year: positiveInteger range(2013,2023)
country: any("BEL","BGR","DNK","DEU","EST","IRL","GRC","ESP","FRA","HRV","ITA","CYP","LVA","LTU","MLT","NLD","POL","PRT","ROU","SVN","FIN","SWE","GBR","ENG","GBC","GBG","GBJ","IOM","NIR","SCO")
